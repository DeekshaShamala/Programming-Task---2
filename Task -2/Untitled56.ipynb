        "def average(array):\n",
        "    # your code goes here\n",
        "    d = set(array)\n",
        "    \n",
        "    return (sum(d)/len(d))\n",
        "    \n",
        "\n",
        "if __name__ == '__main__':\n",
        "    n = int(input())\n",
